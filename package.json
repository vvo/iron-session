{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "iron-session",
  "version": "6.3.1",
  "description": "Stateless session utility using signed and encrypted cookies to store data.",
  "keywords": [
    "session",
    "cookie",
    "encryption",
    "security",
    "next.js",
    "express",
    "koa"
  ],
  "bugs": "https://github.com/vvo/iron-session/issues",
  "repository": "github:vvo/iron-session",
  "funding": [
    "https://github.com/sponsors/vvo",
    "https://github.com/sponsors/brc-dd"
  ],
  "license": "MIT",
  "author": "Vincent Voyer <vincent@codeagain.com> (https://github.com/vvo)",
  "sideEffects": false,
  "type": "module",
  "exports": {
    "import": {
      "types": "./dist/index.d.ts",
      "deno": "./dist/index.js",
      "browser": "./dist/index.js",
      "default": "./dist/index.node.js"
    },
    "require": {
      "types": "./dist/index.node.d.cts",
      "default": "./dist/index.node.cjs"
    }
  },
  "main": "./dist/index.node.cjs",
  "files": [
    "dist/*"
  ],
  "scripts": {
    "build": "pnpm i && tsup && cp dist/index.d.ts dist/index.node.d.cts",
    "lint": "pnpm build && pnpm lint:only",
    "lint:only": "lefthook run pre-commit && publint",
    "release": "pnpm lint && release-it"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.1",
    "@commitlint/config-conventional": "^17.6.1",
    "@release-it/conventional-changelog": "^5.1.1",
    "@types/cookie": "^0.5.1",
    "@types/node": "^18.15.13",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "cookie": "^0.5.0",
    "eslint": "^8.39.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-node": "^0.3.7",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-security": "^1.7.1",
    "iron-webcrypto": "^0.7.0",
    "lefthook": "^1.3.10",
    "prettier": "^2.8.7",
    "prettier-plugin-packagejson": "^2.4.3",
    "publint": "^0.1.11",
    "release-it": "^15.10.1",
    "tsup": "^6.7.0",
    "typescript": "^5.0.4"
  },
  "packageManager": "pnpm@8.3.1",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  }
}
